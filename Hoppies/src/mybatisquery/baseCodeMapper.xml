<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
				"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="front.baseCode.dao.BaseCodeDAO">

	<select id="selectAreaAll" parameterType="BaseCodeVO" resultType="BaseCodeVO">
		SELECT *
		FROM BASE_CODE
		WHERE PARNTS_CODE = 1
	</select>
	
	<select id="selectHobyyAll"  parameterType="BaseCodeVO" resultType="BaseCodeVO">
		SELECT *
		FROM BASE_CODE
		WHERE PARNTS_CODE = 2
	</select>

</mapper>	